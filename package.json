{
  "name": "merouter",
  "version": "1.0.0",
  "description": "a simple route",
  "main": "./dist/router.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "build": "rm -rf ./dist && rollup --config rollup.config.js",
    "build:ts": "rm -rf ./dist && ./node_modules/typescript/bin/tsc",
    "tslint": "./node_modules/tslint/bin/tslint --config ./tslint.json ./src/**/*.ts",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foc-me/merouter.git"
  },
  "keywords": [
    "route"
  ],
  "author": "lss",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foc-me/merouter/issues"
  },
  "homepage": "https://github.com/foc-me/merouter#readme",
  "lint-staged": {
    "./src/**/*ts": [
      "tslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "rollup": "^2.35.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
